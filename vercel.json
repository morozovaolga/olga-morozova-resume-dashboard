{
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "src": "/olga/portfolio/(.*)",
      "dest": "/olga/portfolio/"
    },
    {
      "src": "/olga/portfolio",
      "dest": "/olga/portfolio/"
    }
  ],
  "rewrites": [
    { "source": "/olga/portfolio/(.*)", "destination": "/olga/portfolio/" },
    { "source": "/olga/portfolio", "destination": "/olga/portfolio/" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}


